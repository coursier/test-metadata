  <dependencies>
    <dependency>
      <groupId>org.mvnpm</groupId>
      <artifactId>cliui</artifactId>
      <version>${org.mvnpm-cliui.version}</version>
    </dependency>
    <dependency>
      <groupId>org.mvnpm</groupId>
      <artifactId>escalade</artifactId>
      <version>${org.mvnpm-escalade.version}</version>
    </dependency>
    <dependency>
      <groupId>org.mvnpm</groupId>
      <artifactId>get-caller-file</artifactId>
      <version>${org.mvnpm-get-caller-file.version}</version>
    </dependency>
    <dependency>
      <groupId>org.mvnpm</groupId>
      <artifactId>require-directory</artifactId>
      <version>${org.mvnpm-require-directory.version}</version>
    </dependency>
    <dependency>
      <groupId>org.mvnpm</groupId>
      <artifactId>string-width</artifactId>
      <version>${org.mvnpm-string-width.version}</version>
    </dependency>
    <dependency>
      <groupId>org.mvnpm</groupId>
      <artifactId>y18n</artifactId>
      <version>${org.mvnpm-y18n.version}</version>
    </dependency>
    <dependency>
      <groupId>org.mvnpm</groupId>
      <artifactId>yargs-parser</artifactId>
      <version>${org.mvnpm-yargs-parser.version}</version>
    </dependency>
  </dependencies>
